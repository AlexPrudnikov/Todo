@model IEnumerable<UserRolesModel>
@{
    Int32 index = 0;
}
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Roles</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            @foreach (var user in item.User)
            {
                index = index + 1;
                <tr>
                    <td>@index</td>
                    <td>@user.Id</td>
                    <td>@user.UserName</td>
                    <td identity-role="@item.Roles"></td>
                </tr>
            }
        }
    </tbody>
</table>
